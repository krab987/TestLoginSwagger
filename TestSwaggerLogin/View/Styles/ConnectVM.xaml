<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModel="clr-namespace:TestSwaggerLogin.ViewModel"
                    xmlns:view="clr-namespace:TestSwaggerLogin.View">
    <DataTemplate DataType="{x:Type viewModel:MainViewModel}">
        <view:MainWindow />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:LoginViewModel}">
        <view:LoginPage />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:InfoViewModel}">
        <view:InfoPage />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:AllInfoViewModel}">
        <view:AllInfoPage />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:NameSurnameViewModel}">
        <view:NameSurnamePage />
    </DataTemplate>

</ResourceDictionary>