<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:pages="clr-namespace:TestSwaggerLogin.View.Pages"
                    xmlns:viewModel="clr-namespace:TestSwaggerLogin.ViewModel">
    <DataTemplate DataType="{x:Type viewModel:LoginViewModel}">
        <pages:LoginPage />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:InfoViewModel}">
        <pages:InfoPage></pages:InfoPage>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:AllInfoViewModel}">
        <pages:AllInfoPage />
    </DataTemplate> 
    <DataTemplate DataType="{x:Type viewModel:NameSurnameViewModel}">
        <pages:NameSurnamePage />
    </DataTemplate>
    
</ResourceDictionary>