<Page x:Class="TestSwaggerLogin.View.Pages.AllInfoPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:TestSwaggerLogin.View.Pages"
      xmlns:viewModel="clr-namespace:TestSwaggerLogin.ViewModel"
      mc:Ignorable="d"
      Title="AllInfoPage" Height="450" Width="800">
    
    <Grid>
        <StackPanel>
            <TextBlock Text="{Binding CurrentUser.name}"></TextBlock>
            <TextBlock Text="{Binding CurrentUser.surname}"></TextBlock>
            <TextBlock Text="{Binding CurrentUser.roles}"></TextBlock>
            <TextBlock Text="{Binding CurrentUser.number}"></TextBlock>
            <TextBlock Text="{Binding CurrentUser.authenticated}"></TextBlock>
            <TextBlock Text="{Binding CurrentUser.isvalid}"></TextBlock>
        </StackPanel>
    </Grid>
</Page>
