<Page x:Class="TestSwaggerLogin.View.Pages.NameSurnamePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:TestSwaggerLogin.View.Pages"
      xmlns:viewModel="clr-namespace:TestSwaggerLogin.ViewModel"
      mc:Ignorable="d"
      Title="NameSurnamePage" Height="450" Width="800">
   
    <Grid>
        <StackPanel>
            <TextBlock Text="{Binding Name}"></TextBlock>
            <TextBlock Text="{Binding Surname}"></TextBlock>
        </StackPanel>
    </Grid>
</Page>
